{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": "please_use_yarn_instead"
  },
  "scripts": {
    "dev": "run-p next:dev typed-scss-modules",
    "next:dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "ssg": "next build && next export",
    "ssg-start": "npx http-server ./out",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typed-scss-modules": "typed-scss-modules src -w",
    "clean": "rm -rf node_modules yarn.lock .next"
  },
  "dependencies": {
    "@apollo/client": "3.11.8",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "framer-motion": "11.11.9",
    "graphql": "16.9.0",
    "modern-css-reset": "1.4.0",
    "next": "12.3.4",
    "next-pwa": "5.6.0",
    "next-seo": "6.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-intersection-observer": "9.13.1",
    "react-markdown": "9.0.1",
    "react-use": "17.5.1",
    "three": "0.169.0",
    "vanta": "0.5.24"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.7",
    "@types/node": "18.19.59",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/three": "0.169.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.57.1",
    "eslint-config-next": "13.5.7",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.2",
    "npm-run-all2": "5.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-sort-json": "3.1.0",
    "sass": "1.80.4",
    "typed-scss-modules": "7.1.4",
    "typescript": "5.6.3"
  },
  "packageManager": "yarn@3.8.6"
}
